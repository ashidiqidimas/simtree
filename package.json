{
  "name": "@ashidiqidimas/simtree",
  "version": "0.4.0",
  "description": "Manage git worktrees with automatic iOS simulator assignment",
  "type": "module",
  "bin": {
    "simtree": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "tsup",
    "link": "pnpm build && pnpm link --global",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "worktree",
    "git",
    "simulator",
    "xcode",
    "ios",
    "cli"
  ],
  "author": "ashidiqidimas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashidiqidimas/simtree.git"
  },
  "packageManager": "pnpm@10.9.0",
  "dependencies": {
    "@inquirer/prompts": "^8.3.0",
    "commander": "^14.0.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
